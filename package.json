{
  "name": "react-native-big-dipper",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ipse": "react-native run-ios --simulator='iPhone SE (3rd generation)'",
    "start": "react-native start",
    "startr": "react-native start --reset-cache",
    "pod": "npx pod-install",
    "test": "jest",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "prestart": "sb-rn-get-stories",
    "storybook-watcher": "sb-rn-watcher",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/datetimepicker": "^6.5.0",
    "@react-native-community/slider": "^4.3.2",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-ondevice-actions": "^6.0.1-beta.8",
    "@storybook/addon-ondevice-backgrounds": "^6.0.1-beta.8",
    "@storybook/addon-ondevice-controls": "^6.0.1-beta.8",
    "@storybook/addon-ondevice-notes": "^6.0.1-beta.8",
    "@storybook/react-native": "^6.0.1-beta.8",
    "react": "18.2.0",
    "react-native": "0.70.3",
    "react-native-bars": "^1.3.0",
    "react-native-bootsplash": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/runtime": "^7.19.4",
    "@react-native-community/eslint-config": "^3.1.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.1.2",
    "@types/react-native": "^0.70.4",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-jest": "^29.1.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "0.73.1",
    "prettier": "^2.7.1",
    "react-native-safe-area-context": "^4.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npm run lint --fix-dry-run"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
